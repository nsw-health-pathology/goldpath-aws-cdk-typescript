{
  "name": "goldpath-aws-cdk-typescript",
  "version": "1.0.0",
  "description": "A generic TypeScript AWS CDK project that follows the Golden Path guide.",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/nsw-health-pathology/goldpath-aws-cdk-typescript.git"
  },
  "keywords": [
    "golden",
    "path",
    "goldenpath",
    "aws-cdk",
    "aws",
    "typescript",
    "cdk",
    "cloud",
    "amazon",
    "developer",
    "kit",
    "nswhp"
  ],
  "homepage": "https://github.com/nsw-health-pathology/goldpath-aws-cdk-typescript#readme",
  "bin": {
    "cdk": "dist/cdk-project.js"
  },
  "scripts": {
    "cdk": "node ./node_modules/aws-cdk/bin/cdk.js",
    "tsc": "node ./node_modules/typescript/bin/tsc",
    "lint": "eslint -c .eslintrc.json",
    "build": "npm run tsc --",
    "watch": "npm run tsc -- -w",
    "test": "jest",
    "deploy": "cdk deploy",
    "diff": "cdk diff",
    "ci": "npm run lint && npm run test",
    "synth": "cdk synth",
    "license-check": "license-checker --summary --production --excludePrivatePackages --onlyAllow Apache-2.0;MIT;ISC;BSD;BSD-2-Clause;BSD-3-Clause;MIT*;CC0-1.0;CC-BY-3.0;CC-BY-4.0;BSD*;0BSD;Public Domain;AFLv2.1;"
  },
  "devDependencies": {
    "@types/jest": "^29.2.5",
    "@types/node": "18.11.18",
    "@typescript-eslint/eslint-plugin": "^5.57.1",
    "@typescript-eslint/parser": "^5.57.1",
    "aws-cdk": "^2.72.0",
    "eslint": "^8.37.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-prettier": "^4.2.0",
    "prettier": "^2.8.7",
    "jest": "^29.3.1",
    "license-checker": "^25.0.1",
    "ts-jest": "^29.0.3",
    "ts-node": "^10.0.0",
    "typescript": "^4.9.3"
  },
  "dependencies": {
    "aws-cdk-lib": "^2.72.0",
    "constructs": "^10.1.301"
  }
}